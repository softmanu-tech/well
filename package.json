{
  "devDependencies": {
    "concurrently": "^8.2.2",
    "ts-node": "^10.9.2"
  },
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd portfolio && npm run dev",
    "dev:backend": "cd quote-request-backend && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd portfolio && npm run build",
    "build:backend": "cd quote-request-backend && npm run build"
  },
  "dependencies": {
    "cors": "^2.8.5"
  }
}
